n  return (\n    <div className='weather-forecast__wrapper'>\n      <h2 className='weather-forecast__title'>WEATHER FORECAST</h2>\n      <Row>\n        <Col md={{span: 4, offset: 1}}>\n          <SearchField\n            className='weather-forecast__input'\n            onChange={_onType}\n            onSelect={_onSelect}\n            value={keyword}\n            queryResults={queryResults}\n            loading={locationLoading}\n          />\n        </Col>\n      </Row>\n      <Row>\n        <Col md={{span: 10, offset: 1}}>\n          <WeatherBlock info={weatherInfo}/>\n        </Col>\n      </Row>\n      {loading &&\n        <LoadingSpinner\n          loaderClassName='loading-spinner__loader--weather-block'\n          wrapperClassName='loading-spinner__wrapper--weather-block'\n        />\n      }\n    </div>\n  )\n}\n\nexport default WeatherForecastCard","/Users/quyenlam/Documents/Personal/Assignment/weather-forecast/src/core/constants.js",[],"/Users/quyenlam/Documents/Personal/Assignment/weather-forecast/src/reportWebVitals.js",[],["67","68"],"/Users/quyenlam/Documents/Personal/Assignment/weather-forecast/src/reducers/index.js",[],"/Users/quyenlam/Documents/Personal/Assignment/weather-forecast/src/components/weather-block/index.js",["69"],"/Users/quyenlam/Documents/Personal/Assignment/weather-forecast/src/actions/location.js",[],"/Users/quyenlam/Documents/Personal/Assignment/weather-forecast/src/reducers/weather.js",["70","71"],"/Users/quyenlam/Documents/Personal/Assignment/weather-forecast/src/components/search-field/index.js",[],"/Users/quyenlam/Documents/Personal/Assignment/weather-forecast/src/actions/weather.js",[],"/Users/quyenlam/Documents/Personal/Assignment/weather-forecast/src/reducers/location.js",["72"],"/Users/quyenlam/Documents/Personal/Assignment/weather-forecast/src/components/weather-block/weather-card.js",["73","74","75"],"import React, {useEffect, useState} from 'react'\nimport classNames from 'classnames';\nimport PropTypes from 'prop-types'\n